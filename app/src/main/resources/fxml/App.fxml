<AnchorPane xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="xyz.gnas.elif.app.controllers.AppController">
    <children>
        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <HBox fx:id="hboExplorer" VBox.vgrow="ALWAYS" />
            <ScrollPane maxHeight="100.0">
               <content>
                  <VBox fx:id="vboOperations" />
               </content>
            </ScrollPane>
            </children>
        </VBox>
    </children>
</AnchorPane>
